/**index.wxss**/
.container {
  background-color: rgb(245, 245, 245);
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
} 

.scrollarea{
  height: 100vh;
  padding-bottom: calc(env(safe-area-inset-bottom)+100px); /* 兼容iOS 11.0+ */
  padding-bottom: calc(constant(safe-area-inset-bottom)+100px); /* 兼容iOS 11.0以下 */
}

.searchBtn{
  padding:0 20rpx;
  margin: 0 10rpx;
  color: white ;
  font-weight: 600;
  background-color: red; 
  border-radius: 10rpx;
}

.gridStyle{
  border-radius: 32rpx;
}




