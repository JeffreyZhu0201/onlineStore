<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jeffrey.onlinestorebe.mapper.LocationMapper">

    <insert id="insertLocation" useGeneratedKeys="true" keyProperty="id" parameterType="com.jeffrey.onlinestorebe.entity.locationEntity.Location">
        insert into location (id,position, phone,name,user_id)
        values ( #{id},#{position}, #{phone},#{name},#{user_id})
    </insert>

</mapper>